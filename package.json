{
  "name": "as-helloworld",
  "version": "1.0.0",
  "description": "AssemblyScript Hello World example thingy",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "asbuild:untouched": "asc wasm/assembly/index.ts -b wasm/build/untouched.wasm -t wasm/build/untouched.wat --validate --sourceMap --debug",
    "asbuild:optimized": "asc wasm/assembly/index.ts -b wasm/build/optimized.wasm -t wasm/build/optimized.wat --validate --sourceMap --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "start": "ts-node src",
    "build": "tsc",
    "serve": "http-server -f /index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jirinrin/AssemblyScript-HelloWorld.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jirinrin/AssemblyScript-HelloWorld/issues"
  },
  "homepage": "https://github.com/Jirinrin/AssemblyScript-HelloWorld#readme",
  "devDependencies": {
    "assemblyscript": "^0.9.2",
    "@types/node": "^13.9.0",
    "ts-node": "^8.6.2",
    "tsc": "^1.20150623.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.9.2",
    "http-server": "^0.12.1"
  }
}
