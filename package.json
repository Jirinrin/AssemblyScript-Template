  "description": "AssemblyScript Hello World example thingy",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "asbuild:untouched": "asc wasm/assembly/index.ts -b wasm/build/untouched.wasm -t wasm/build/untouched.wat --validate --sourceMap --debug",
